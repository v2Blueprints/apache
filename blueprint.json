{
	"about": {
		"title": "apache",
		"explanation": "apache web Server"
	},
	"configuration": {
		"ServerAdmin": "webmaster@^^domain.name^^",
		"ServerName": "^^domain.qualified_name^^"
	},
	"binding_target": {
		"ContainerName": "^^binder.identifier^^",
		"port": 80
	},
	"images": [
		{
		 "type": "docker",
		 "image": "engines/debian"
		}
	],
	"bindings": [
		{
			"type": "embed",
			"runtime": "lxd",
			"target": {
				"repository": "https://github.com/v2Blueprints/enginesd_debian_base"
			}
		}
	],
	"system_packages": {
		"adds": [
			"apache2"
		]
	},
	"executions": [
		{
			"type": "docker",
			"CMD": "/usr/sbin/apachectl start"
		}
	]
}
